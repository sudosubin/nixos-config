{
    "workbench.colorTheme": "One Dark Pro",
    "editor.fontFamily": "'FiraMono Nerd Font Mono', 'SF Pro Text'",
    "editor.fontSize": 13,
    "debug.console.fontSize": 1,
    "debug.console.fontFamily": "'FiraMono Nerd Font Mono', 'SF Pro Text'",
    "markdown.preview.fontSize": 14,
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontFamily": "'FiraMono Nerd Font Mono', 'SF Pro Text'",
    "markdown.preview.fontFamily": "'SF Pro Text', 'FiraMono Nerd Font Mono'",
    "editor.rulers": [
        120
    ],
    "workbench.tree.indent": 16,
