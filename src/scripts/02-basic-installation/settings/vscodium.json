{
    "workbench.colorTheme": "One Dark Pro",
    "oneDarkPro.editorTheme": "Onedark Pro",
    "markdown.preview.fontFamily": "'SF Pro Text'",
    "editor.fontFamily": "'FiraMono Nerd Font Mono', 'SF Pro Text'",
    "debug.console.fontFamily": "'FiraMono Nerd Font Mono', 'SF Pro Text'",
    "terminal.integrated.fontFamily": "'FiraMono Nerd Font Mono', 'SF Pro Text'",
    "editor.fontSize": 13,
    "debug.console.fontSize": 13,
    "markdown.preview.fontSize": 13,
    "terminal.integrated.fontSize": 13,
    "editor.rulers": [
        120
    ],
    "editor.minimap.renderCharacters": false,
    "workbench.tree.indent": 16,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.startupEditor": "newUntitledFile",
    "material-icon-theme.activeIconPack": "react_redux",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.showWelcomeMessage": false,
    "gitlens.currentLine.format": "${author} â€¢ ${date}, ${message}",
    "gitlens.currentLine.dateFormat": "YYYY-MM-DD",
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    "window.zoomLevel": 0,
    "shellcheck.customArgs": [
        "-x"
    ],
    "trailing-spaces.backgroundColor": "rgba(255, 0, 0, 0.2)",
    "trailing-spaces.borderColor": "rgba(255, 100, 100, 0.1)",
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--max-line-length=120"
    ],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/venv/*/**": true
    },
    "customizeUI.font.regular": "SF Pro Text",
    "customizeUI.font.monospace": "FiraMono Nerd Font Mono",
    "customizeUI.stylesheet": {
        ".search-widget textarea, .find-widget textarea, .search-view .results .match": "font-size: 12px; font-family: 'FiraMono Nerd Font Mono' !important;",
        ".editor-container": "-webkit-font-smoothing: antialiased;"
    },
    "telemetry.enableCrashReporter": false,
    "gitlens.advanced.telemetry.enabled": false
}