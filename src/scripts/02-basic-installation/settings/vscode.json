{
    "oneDarkPro.editorTheme": "onedarkPro",
    "markdown.preview.fontFamily": "'SF Pro Text'",
    "editor.fontFamily": "'FiraMono Nerd Font Mono', 'SF Pro Text'",
    "debug.console.fontFamily": "'FiraMono Nerd Font Mono', 'SF Pro Text'",
    "terminal.integrated.fontFamily": "'FiraMono Nerd Font Mono', 'SF Pro Text'",
    "editor.fontSize": 13,
    "debug.console.fontSize": 13,
    "markdown.preview.fontSize": 13,
    "terminal.integrated.fontSize": 13,
    "window.zoomLevel": 0,
    "terminal.explorerKind": "external",
    "terminal.external.linuxExec": "st",
    "editor.rulers": [
        120
    ],
    "editor.minimap.renderCharacters": false,
    "editor.semanticHighlighting.enabled": false,
    "workbench.colorTheme": "One Dark Pro",
    "workbench.tree.indent": 16,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.iconTheme": "material-icon-theme",
    "material-icon-theme.activeIconPack": "react_redux",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.showWelcomeMessage": false,
    "material-icon-theme.files.associations": {
        "stories.tsx": "storybook"
    },
    "gitlens.currentLine.format": "${author} â€¢ ${date}, ${message}",
    "gitlens.currentLine.dateFormat": "YYYY-MM-DD",
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": false,
    "shellcheck.customArgs": [
        "-x"
    ],
    "trailing-spaces.backgroundColor": "rgba(255, 0, 0, 0.2)",
    "trailing-spaces.borderColor": "rgba(255, 100, 100, 0.1)",
    "python.showStartPage": false,
    "python.languageServer": "Pylance",
    "python.analysis.typeCheckingMode": "off",
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--max-line-length=120"
    ],
    "autoDocstring.generateDocstringOnEnter": true,
    "autoDocstring.guessTypes": true,
    "files.watcherExclude": {
        "**/.git/*/**": true,
        "**/node_modules/*/**": true,
        "**/venv/*/**": true,
        "**/.venv/*/**": true
    },
    "explorer.compactFolders": false,
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "editor.trimAutoWhitespace": true,
    "editor.insertSpaces": true,
    "[python]": {
        "editor.tabSize": 4,
        "files.insertFinalNewline": true
    },
    "[javascript]": {
        "editor.tabSize": 2
    },
    "[typescript]": {
        "editor.tabSize": 2
    },
    "[javascriptreact]": {
        "editor.tabSize": 2
    },
    "[typescriptreact]": {
        "editor.tabSize": 2
    },
    "[html]": {
        "editor.tabSize": 2
    },
    "customizeUI.font.regular": "SF Pro Text",
    "customizeUI.font.monospace": "FiraMono Nerd Font Mono",
    "customizeUI.stylesheet": {
        ".search-widget textarea, .find-widget textarea, .search-view .results .match": "font-size: 12px; font-family: 'FiraMono Nerd Font Mono' !important;",
        ".editor-container": "-webkit-font-smoothing: antialiased;"
    },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "string.quoted.docstring.multi.python",
                    "string.quoted.docstring.multi.python punctuation.definition.string.begin.python",
                    "string.quoted.docstring.multi.python punctuation.definition.string.end.python"
                ],
                "settings": {
                    "foreground": "#7f848e"
                }
            }
        ]
    },
    "highlight.regexes": {
        "(Examples?|Attributes|Todos?|Args|Returns?|Raises|Yields|Note|\\.\\.\\ _.*)(:)": {
            "regexFlags": "g",
            "filterLanguageRegex": "python",
            "decorations": [
                {"color": "#c678dd", "textDecoration": ";font-style: italic;"},
                {"color": "#7f848e"}
            ]
        }
    },
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "gitlens.advanced.telemetry.enabled": false,
    "extensions.ignoreRecommendations": true,
    "update.mode": "none"
}